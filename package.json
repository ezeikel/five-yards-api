{
  "name": "five-yards-api",
  "version": "1.0.0",
  "author": "hi@ezeikel.dev",
  "license": "ISC",
  "scripts": {
    "bake-env": "node ./scripts/process.env-generator.js",
    "dev": "NODE_ENV=test yarn bake-env && yarn prisma:generate && sls offline --aws-profile ezeikel-admin",
    "lint": "jest --config test/jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|css|md|mdx|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "deploy": "NODE_ENV=production sls deploy --aws-profile ezeikel-admin",
    "undeploy": "sls remove --aws-profile ezeikel-admin",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prepare": "husky install"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "**/*.+(ts|js|json|md)": [
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@sentry/serverless": "^6.17.9",
    "apollo-server-lambda": "^3.6.3",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "graphql-upload": "^13.0.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "nodemailer": "^6.7.2",
    "path": "^0.12.7",
    "stripe": "^8.202.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/graphql-upload": "8.0.11",
    "@types/jsonwebtoken": "8.5.8",
    "@types/md5": "2.3.2",
    "@types/node": "17.0.18",
    "@types/nodemailer": "6.4.4",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "babel-loader": "8.2.3",
    "cache-loader": "4.1.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "is-ci-cli": "2.2.0",
    "jest": "27.5.1",
    "jest-in-case": "1.0.2",
    "jest-runner-eslint": "1.0.0",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "prisma": "3.9.2",
    "serverless-domain-manager": "6.0.2",
    "serverless-offline": "8.5.0",
    "serverless-offline-variables": "0.1.0",
    "serverless-webpack": "5.6.1",
    "serverless-webpack-prisma": "^1.0.9",
    "typescript": "4.5.5",
    "webpack": "5.69.1",
    "webpack-node-externals": "3.0.0"
  }
}
