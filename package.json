{
  "name": "five-yards-api",
  "version": "1.0.0",
  "author": "hi@ezeikel.dev",
  "license": "ISC",
  "scripts": {
    "build": "babel src --config-file './.babelrc.json' --extensions .js,.ts --out-dir dist",
    "lint": "jest --config test/jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|css|md|mdx|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "start": "node dist/server.js",
    "dev": "yarn bake-env && yarn start:dev",
    "start:dev": "nodemon 'src/server.ts' --exec babel-node --config-file './.babelrc.json' --extensions .js,.ts -r dotenv/config",
    "bake-env": "node ./scripts/process.env-generator.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "**/*.+(ts|js|json|md)": [
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@sentry/node": "6.8.0",
    "apollo-server-express": "^2.25.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.3.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.3.0",
    "md5": "^2.3.0",
    "mongoose": "^5.13.3",
    "mongoose-mongodb-errors": "^0.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.2",
    "passport-local-mongoose": "^6.1.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "request-promise": "^4.2.6",
    "stripe": "^8.160.0",
    "util": "^0.12.4",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-eslint": "^10.1.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "inquirer": "^8.1.1",
    "is-ci": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.0.6",
    "jest-in-case": "^1.0.2",
    "jest-runner-eslint": "^0.10.1",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "replace-in-file": "^6.2.0",
    "typescript": "^4.3.5"
  }
}
